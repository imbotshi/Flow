{
  "name": "vania-flow-backend",
  "version": "1.0.0",
  "description": "Backend Node.js avec Express et Mongoose pour Vania Flow",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-mongoose": "node scripts/test-mongoose-connection.js",
    "install-db": "node scripts/install-database.js",
    "setup": "npm install && node scripts/install-database.js",
    "test-db": "node scripts/test-database.js",
    "full-setup": "npm install && node scripts/install-database.js && node scripts/test-database.js",
    "configure": "node scripts/setup-env.js",
    "load-data": "node scripts/load-sample-data.js",
    "check-data": "node scripts/check-data.js",
    "complete-setup": "npm install && node scripts/install-database.js && node scripts/load-sample-data.js && node scripts/test-database.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "mongodb": "^6.3.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongoose",
    "mongodb",
    "vania-flow",
    "backend"
  ],
  "author": "Vania Flow Team",
  "license": "MIT"
} 